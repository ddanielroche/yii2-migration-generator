{
    "name": "ddanielroche/yii2-migration-generator",
    "description": "Generate migration files for one table, comma separated list of tables, part of table name or all tables.",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "extension",
        "gii",
        "migration"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Dany Daniel Roche",
            "email": "ddanielroche@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "^2.0.6"
    },
    "require-dev": {
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-coding-standards": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "ddanielroche\\migration\\": ""
        }
    },
    "extra": {
        "bootstrap": "ddanielroche\\migration\\Bootstrap"
    }
}
